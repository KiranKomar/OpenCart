<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="TestSuite" >
	
	<groups>
		<run>
			<include name="master" />
			<!--<include name="Sanity" /> -->
			<!--<include name="Regression" />-->
			<!--<exclude name="" /> -->
		<!--	<include name="Datadriven" />-->
		</run>
	</groups>
	
	<listeners>
		<listener class-name="Utilites.ExtentReporter" />
	</listeners>
	
	<test name="windows-chrome" enabled="true">
		<parameter name="os" value="window"/>
		<parameter name="browser" value="chrome"/>
		<classes>
			<class name="Testcases.TestCase_1"/>
			<class name="Testcases.TC2_DDT"/>
		
		</classes>
	</test>
	
	<test name="windows-edge" enabled="false">
		<parameter name="os" value="window"/>
		<parameter name="browser" value="edge"/>
		<classes>
			<class name="Testcases.TestCase_1"/>
		<class name="Testcases.TC2_DDT"/>
		</classes>
	</test>
		
</suite>